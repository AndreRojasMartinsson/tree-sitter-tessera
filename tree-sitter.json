{
	"$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
	"grammars": [
		{
			"name": "tessera",
			"camelcase": "Tessera",
			"title": "Tessera",
			"scope": "source.tessera",
			"path": ".",
			"file-types": ["tes", "tessera"],
			"class-name": "TreeSitterTessera",
			"highlights": ["queries/highlights.scm"],
			"injections": ["queries/injections.scm"],
			"tags": ["queries/tags.scm"],
			"injection-regex": "tessera"
		}
	],
	"metadata": {
		"version": "0.1.0",
		"license": "MIT",
		"description": "Tessera grammar for tree-sitter",
		"authors": [
			{
				"name": "AndreRojasMartinsson"
			}
		],
		"links": {
			"repository": "https://github.com/tree-sitter/tree-sitter-tessera"
		}
	},
	"bindings": {
		"c": true,
		"go": true,
		"node": true,
		"python": true,
		"rust": true,
		"swift": true,
		"zig": false
	}
}
